REACT_APP_PRIVACY_POLICY_URL=
REACT_APP_TERMS_OF_SERVICE_URL=

# File Upload Limits
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=".jpg,.jpeg,.png,.bmp"